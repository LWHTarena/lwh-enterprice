<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lwhtarena.company.web.entities">
	<class name="TempletDownCode" table="templet_downcode" dynamic-update="true">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="code" not-null="true" />
		<property name="templetID" />
		<property name="typeNum" />
		<property name="createDate" />
		<property name="downDate" />
		<property name="createIP" />
		<property name="createHost" />
		<property name="downIP" />
		
					
	</class>
</hibernate-mapping>
