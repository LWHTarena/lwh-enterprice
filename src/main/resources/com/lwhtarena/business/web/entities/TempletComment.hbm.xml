<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lwhtarena.company.web.entities">
	<class name="TempletComment" table="templet_comment" dynamic-update="true">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="formCode" type="text" />
		<property name="itemLoopCode" type="text" />
		<property name="actPassCode" type="text" />
		<property name="actDelCode" type="text" />
		<property name="actReplyCode" type="text" />
			
	</class>
</hibernate-mapping>
