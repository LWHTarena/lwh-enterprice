<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lwhtarena.company.web.entities">
	<class name="VisitorsBook" table="visitors_book" dynamic-update="true">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		
		<property name="objType" />
		<property name="objID" />
		<property name="objTitle" />
		<property name="viewsTotal" />
		<property name="viewsSub" />
		<property name="viewsOwn" />
		<property name="ipTotal" />
		<property name="ipSub" />
		<property name="ipOwn" />

	</class>
</hibernate-mapping>
